<?xml version="1.0" encoding="utf-8"?>
<resources>
<declare-styleable name="ZukiMenu">
    <attr name="wbackground" format="color" />
    <attr name="indicator" format="enum" >
        <enum name="left" value="0"/>
        <enum name="right" value="1"/>
    </attr>
    <attr name="title" format="string" />
    <attr name="strColor" format="color" />
    <attr name="strSize" format="integer" />
    <attr name="indicatorColor" format="color" />
    <attr name="indicatorSize" format="integer" />
    <attr name="android:padding"/>
    <attr name="fontname" format="string"/>
</declare-styleable>

<declare-styleable name="HeaderGroup">
</declare-styleable>

<declare-styleable name="Expandable">
    <attr name="headerBg" format="color"/>
    <attr name="containerBg" format="color" />
    <attr name="header" format="string"/>
    <attr name="headerfontsize" format="float" />
    <attr name="PaddingStart" format="integer"/>
</declare-styleable>
    
<declare-styleable name="ListItem">
    <attr name="iconSrc" format="string"/>
    <attr name="title" format="string"/>
    <attr name="inputVisible" format="boolean"/>
    <attr name="containerBg" format="color"/>
    <attr name="type" format="enum">
        <enum name="searchItem" value="0"/>
        <enum name="eventItem" value="1"/>
    </attr>
</declare-styleable>
</resources>